{
    // Window
    "window.zoomLevel": -1,
    "window.restoreWindows": "all",
    // Explorer
    "explorer.confirmDragAndDrop": false,
    // Files
    "files.eol": "\n",
    "files.trimTrailingWhitespace": true,
    // Workbench
    "workbench.iconTheme": "vscode-icons",
    "workbench.fontAliasing": "auto",
    "workbench.editor.showIcons": true,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorTheme": "Community Material Theme Darker High Contrast",
    "workbench.colorCustomizations": {
        "activityBarBadge.background": "#00BCD4",
        "list.activeSelectionForeground": "#00BCD4",
        "list.inactiveSelectionForeground": "#00BCD4",
        "list.highlightForeground": "#00BCD4",
        "scrollbarSlider.activeBackground": "#00BCD450",
        "editorSuggestWidget.highlightForeground": "#00BCD4",
        "textLink.foreground": "#00BCD4",
        "progressBar.background": "#00BCD4",
        "pickerGroup.foreground": "#00BCD4",
        "tab.activeBorder": "#00BCD4"
    },
    // Editor
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "selection",
    "editor.selectionHighlight": true,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.wordWrap": "off",
    "editor.fontFamily": "'Fira Code', Monaco",
    "editor.fontWeight": "500",
    "editor.fontLigatures": true,
    "editor.colorDecorators": false,
    "editor.fontSize": 14,
    "editor.maxTokenizationLineLength": 40000,
    "editor.guides.bracketPairs": true,
    "editor.guides.indentation": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.rulers": [
        80,
        120
    ],
    "color-highlight.markRuler": false,
    // Terminal
    "terminal.integrated.fontFamily": "'Fira Code', Monaco",
    "terminal.integrated.fontLigatures": true,
    "terminal.integrated.fontWeight": "500",
    "terminal.integrated.fontSize": 14,
    // HTML
    "html.format.unformatted": "wbr, a, em, i, b, strong, span, code, sup, sub",
    // ESLint
    "eslint.format.enable": true,
    "eslint.run": "onSave",
    "eslint.workingDirectories": [
        {
            "mode": "auto"
        }
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "editor.unicodeHighlight.allowedCharacters": {
        "â€º": true
    },
    "problems.showCurrentInStatus": true,
    "javascript.format.insertSpaceBeforeFunctionParenthesis": false,
    "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
    "javascript.preferences.importModuleSpecifierEnding": "js",
    "javascript.updateImportsOnFileMove.enabled": "always",
    // Emmet
    "emmet.triggerExpansionOnTab": true,
    // Formatting
    "[javascript]": {
        "editor.formatOnSave": false
    },
    "[javascriptreact]": {
        "editor.formatOnSave": false
    },
    "[typescript]": {
        "editor.formatOnSave": false
    },
    "[typescriptreact]": {
        "editor.formatOnSave": false
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "tabnine.experimentalAutoImports": true,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "git.terminalAuthentication": false,
    // MCFS
    "mcfs.notifications": {
        "hasOpenedConnectionManager": false,
        "hasConnectedToMC": false,
        "dontShowConnectionManagerAlert": false,
        "hasShownChangelog": false,
        "hasSeenPromoForVersion": "3.0"
    }
}